{
    "name": "ensue/snap",
    "description": "Modular architecture build on top of laravel for any laravel project",
    "keywords": ["ensue", "snap","module","modular","laravel","archtecture"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ensue\\Snap\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shiva Thapa",
            "email": "shiva.thapa@ensue.us"
        },
        {
            "name": "Bipin Maharjan",
            "email": "bipin.maharjan@ensue.us"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-pdo": "*",
        "composer-runtime-api": "^2.2",
        "laravel/framework": "^10.0",
        "intervention/image": "^2.7",
        "laravel/helpers": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ensue\\Snap\\SnapProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
